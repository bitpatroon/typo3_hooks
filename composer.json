{
  "name": "bitpatroon/typo3_hooks",
  "type": "typo3-cms-extension",
  "description": "Extension to easily add hooks in your own code.",
  "keywords": [
    "typo3",
    "core",
    "hooks"
  ],
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Sjoerd Zonneveld",
      "homepage": "https://www.bitpatroon.nl",
      "role": "Developer",
	  "email": "github@bitpatroon.nl"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.3",
    "typo3/cms-core": ">=7.6.0 < 10.9.99"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Bitpatroon\\Typo3Hooks\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "typo3_hooks"
    }
  }
}
