{
  "name": "bitpatroon/typo3_hooks",
  "type": "typo3-cms-extension",
  "description": "Extension to easily add hooks in your own code.",
  "keywords": [
    "typo3",
    "core",
    "hooks"
  ],
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Sjoerd Zonneveld",
      "homepage": "https://www.bitpatroon.nl",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.0",
    "typo3/cms-core": ">=7.6.0 < 8.7.99"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "BPN\\Typo3Hooks\\": "Classes"
    }
  }
}
